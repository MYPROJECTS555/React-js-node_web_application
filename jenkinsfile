pipeline
{
    agent any
    stages
    {
        stage('code-checkout')
        {
         steps
         {
            git branch : 'main',url : 'https://github.com/MYPROJECTS555/React-js-node_web_application.git'
         }   
        }
        stage ('install-npm')
        {
            steps
            {
                sh 'node -v'
                sh 'npm install'
                sh 'npm run build'

            }
        }
    }
}
